process OnTarget {
label "OnTarget"
    input:
    path(data)
    path(aling)


    output:
    path("*.scored.tsv"), emit: scored
    path("*_alignements.tsv")

"""
source ${params.Python_env}

if [[ ${params.CFD_access} ]]; then
        onTarget.py -I ${data} --CFD -A ${aling}  -O ${data.getSimpleName()}
else
        onTarget.py -I ${data} -A ${aling}  -O ${data.getSimpleName()}
fi

    """
}
